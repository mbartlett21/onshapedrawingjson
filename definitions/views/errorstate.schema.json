{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://drawing-schemas.onshape.com/views/errorstate",
    "description":"Here 0 = OK (healthy),1 = INFO,2 = WARNING,3 = ERROR (dangling or view generation call failed),4 = UNKNOWN",
    "title": "Error state",
    "type": "object",
    "properties": {
        "value": {
            "type": "integer",
            "default": "0"
        },
        "description": {
            "type": "string",
            "default": ""
        }
    },
    "required": ["value","description"]
}